get_secrets_and_generate_docker_compose_file:
	mvn com.odigeo.technology.secrets-plugin:secrets-plugin:1.3.2:generateDockerComposeFile -N -DgenerateDockerComposeFile.vaultUrl="https://vault.services.odigeo.com" -DgenerateDockerComposeFile.environment=dev -DgenerateDockerComposeFile.module=airobot-check-in-gateway